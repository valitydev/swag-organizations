{
  "name": "@vality/swag-organizations",
  "version": "1.0.0",
  "scripts": {
    "start": "redocly preview-docs",
    "build:yaml": "redocly bundle openapi/openapi.yaml -o web_deploy/openapi",
    "build:json": "npm run build:yaml -- --ext json",
    "build": "npm run build:yaml && npm run build:json",
    "validate": "redocly lint",
    "codegen": "vality-openapi generate ./web_deploy/openapi.json"
  },
  "devDependencies": {
    "@redocly/cli": "^1.33.0",
    "@vality/openapi-generator-cli": "2.0.0"
  }
}
